<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="esuquilloSem7.vistas.registro">
    <ContentPage.Content>
        <StackLayout>
            <Label Text ="VENTANA DE REGISTRO" FontSize="20" HorizontalTextAlignment="CENTER"/>
            <Entry x:Name="txtNombre" Placeholder="Ingrese el nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el usuario"/>
            <Entry x:Name="txtContraseña" Placeholder="Ingrese la contraseña"/>
            <Button x:Name="btnInsertar" Text="INSERTAR" Clicked="btnInsertar_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>